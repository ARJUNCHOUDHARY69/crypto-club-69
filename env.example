# Dropbox Integration Environment Variables
# Copy this file to .env.local and add your actual token

# Dropbox Access Token (get from https://www.dropbox.com/developers/apps)
DROPBOX_ACCESS_TOKEN=your_actual_token_here

# Optional: CoinGecko API Key (for higher rate limits)
COINGECKO_API_KEY=your_coingecko_api_key_here

# App Configuration
NEXT_PUBLIC_APP_NAME=CRYPTO CLUB 69
NEXT_PUBLIC_APP_VERSION=1.0.0
